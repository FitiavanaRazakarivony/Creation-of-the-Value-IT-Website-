CREATE TABLE Asso_20(
   id_admin INT,
   id_demI INT,
   note DECIMAL(15,2) NOT NULL,
   PRIMARY KEY(id_admin, id_demI),
   FOREIGN KEY(id_admin) REFERENCES admin(id_admin),
   FOREIGN KEY(id_demI) REFERENCES demande_demI(id_demI)
);
